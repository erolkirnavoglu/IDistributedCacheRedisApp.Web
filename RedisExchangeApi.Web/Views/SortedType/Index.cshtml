@model HashSet<string>
<form method="post" asp-action="Add" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name" />
    <input type="text" name="score" />
    <button type="submit">Kaydet</button>
</form>

@foreach (var item in Model)
{
    <h5>@item  <a asp-conroller="SetType" asp-action="Delete" asp-route-name="@item">Sil</a>  </h5>
}